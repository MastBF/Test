{
  "expo": {
    "name": "Take&Go",
    "slug": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/trueLogo_with_black_background_and_rounded_corners.png",
   "scheme": "takeandgo",
    "deeplink": {
      "host": "main"
    }, 
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/trueLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCDWBEASleBbBlbPVHEyO4NuHINBDo41y8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCDWBEASleBbBlbPVHEyO4NuHINBDo41y8"
        }
      },
      "package": "com.mast.aora"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e5264aea-963d-495a-a9cf-ca40a336884d"
      }
    },
    "owner": "mast"
  }
}